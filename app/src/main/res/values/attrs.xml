<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="AdBoxView">
        <attr name="selected_color" format="color" />
        <attr name="unselected_color" format="color" />
        <attr name="duration" format="integer" />
        <attr name="autoPlay" format="boolean" />
    </declare-styleable>

    <declare-styleable name="WheelView">
        <attr name="selected_text_color" format="color" />
        <attr name="unselected_text_color" format="color" />
    </declare-styleable>

    <declare-styleable name="CameraPreview">
        <attr name="framing_rect_width" format="dimension" />
        <attr name="framing_rect_height" format="dimension" />
        <attr name="use_texture_view" format="boolean" />
    </declare-styleable>
</resources>